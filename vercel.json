{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node 16.13.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/*": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}